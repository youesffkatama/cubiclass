{
  "name": "scholar-ai-backend",
  "version": "1.0.0",
  "description": "AI-powered study assistant backend with RAG capabilities",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "worker": "ts-node src/workers/pdf.worker.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "education",
    "rag",
    "vector-search",
    "study-assistant",
    "pdf-processing"
  ],
  "author": "Scholar.AI Team",
  "license": "MIT",
