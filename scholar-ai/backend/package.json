    "@xenova/transformers": "^2.11.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.1.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.7.0",
    "openai": "^4.20.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
